        {
          "datasource": {
            "type": "postgres",
            "uid": "langfuse-postgres"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT \n  t.timestamp,\n  t.name,\n  t.metadata->>'agent' as agent,\n  t.metadata->>'project' as project,\n  COALESCE(SUM(o.total_tokens), 0) as tokens,\n  COALESCE(SUM(COALESCE(o.calculated_total_cost, o.total_cost, 0)), 0)::float as cost,\n  array_to_string(t.tags, ', ') as tags\nFROM traces t\nLEFT JOIN observations o ON o.trace_id = t.id\nWHERE t.timestamp >= NOW() - INTERVAL '7 days'\nGROUP BY t.id, t.timestamp, t.name, t.metadata, t.tags\nORDER BY t.timestamp DESC\nLIMIT 50",
          "refId": "A"
        }
      ],
      "title": "Recent Traces",
      "type": "table"
